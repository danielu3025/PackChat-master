<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/com_facebook_button_send_background_color"
    tools:context="shenkar.koruApps.PackChetApp.TopBarFragment">

    <android.support.v7.widget.Toolbar
        android:id="@+id/navBar"
        android:layout_width="384dp"
        android:layout_height="62dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="0dp"
        android:background="@color/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar5"
        android:layout_width="384dp"
        android:layout_height="39dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="0dp"
        android:background="@color/common_google_signin_btn_text_light_default"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/navBar"
        app:layout_constraintHorizontal_bias="0.333"
        tools:layout_editor_absoluteX="3dp" />

    <Button
        android:id="@+id/folderBt"
        android:layout_width="34dp"
        android:layout_height="31dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/folder"
        app:layout_constraintBottom_toBottomOf="@+id/navBar"
        app:layout_constraintLeft_toRightOf="@+id/chatsBt"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.533" />

    <Button
        android:id="@+id/chatsBt"
        android:layout_width="34dp"
        android:layout_height="31dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/chat"
        app:layout_constraintBottom_toBottomOf="@+id/navBar"
        app:layout_constraintLeft_toRightOf="@+id/menuBt"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.533" />

    <Button
        android:id="@+id/taskBt"
        android:layout_width="34dp"
        android:layout_height="31dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/list"
        app:layout_constraintBottom_toBottomOf="@+id/navBar"
        app:layout_constraintLeft_toRightOf="@+id/folderBt"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.533" />

    <Button
        android:id="@+id/robotbt"
        android:layout_width="34dp"
        android:layout_height="31dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/robot"
        app:layout_constraintBottom_toBottomOf="@+id/navBar"
        app:layout_constraintLeft_toRightOf="@+id/calnderBt"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.533" />

    <Button
        android:id="@+id/menuBt"
        android:layout_width="45dp"
        android:layout_height="36dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/menu"
        app:layout_constraintLeft_toLeftOf="@+id/navBar"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/calnderBt"
        android:layout_width="34dp"
        android:layout_height="31dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/calendar"
        app:layout_constraintBottom_toBottomOf="@+id/navBar"
        app:layout_constraintLeft_toRightOf="@+id/taskBt"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.533" />

    <EditText
        android:id="@+id/groupName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:inputType="text"
        android:selectAllOnFocus="false"
        android:singleLine="true"
        android:text="Name"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="@color/com_facebook_share_button_text_color"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar5"
        app:layout_constraintLeft_toLeftOf="@+id/toolbar5"
        app:layout_constraintRight_toRightOf="parent" />
</android.support.constraint.ConstraintLayout>
